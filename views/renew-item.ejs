<% include header %>

<h2>Resister a new item</h2>
<hr/>

<form method="post" role="form">
  <div class="form-group col-lg-4">
      <label for="name">명칭</label>
      <input type="text" class="form-control" id="name" name="name" placeholder="Entry name" required>
  </div>
  <div class="form-group col-lg-4">
      <label for="price">매매대금</label>
      <input type="number" class="form-control" id="price" name="price" placeholder="Entry price" required>
  </div>
  <div class="form-group row-lg-1">
    <input type="submit" value="Post entry" class="btn btn-primary">
  </div>
</form>

<% include footer %>
