<% include header %>

<h2>Register User Information</h2>
<hr/>

<form method="post" role="form">
  <div class="form-group col-lg-4">
      <label for="name">이름</label>
      <input type="text" class="form-control" id="name" name="name" placeholder="Entry name" required>
  </div>
  <div class="form-group col-lg-4">
      <label for="id">주민등록번호</label>
      <input type="number" class="form-control" id="id" name="id" placeholder="Entry id Number" required>
  </div>
  <div class="form-group col-lg-4">
      <label for="private">private key</label>
      <input type="text" class="form-control" id="private" name="private" placeholder="Entry private key" required>
  </div>
  <div class="form-group row-lg-3">
    <label for="address">주소</label>
    <textarea class="form-control" id="address" name="address" placeholder="거주지를 입력해주세요." rows="3" required>
    </textarea>
  </div>
  <div class="form-group row-lg-1">
    <input type="submit" value="Resister Account" class="btn btn-primary">
  </div>
</form>

<% include footer2 %>
